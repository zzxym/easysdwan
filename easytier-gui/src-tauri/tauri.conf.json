{
  "tauri": {
    "build": {
      "beforeDevCommand": "npm run dev",
      "beforeBuildCommand": "npm run build",
      "devPath": "http://localhost:5173",
      "distDir": "../dist",
      "withGlobalTauri": false
    },
    "bundle": {
      "active": true,
      "productName": "easytier-gui",
      "version": "0.1.0",
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.zzxym.sdwan",
      "longDescription": "A GUI tool to manage \"EasyTier\" based SDWAN connections",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "EasyTier GUI for \"SDWAN\" deployment",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "identifier": "com.zzxym.sdwan"
      },
      "android": {
        "identifier": "com.zzxym.sdwan",
        "minSdkVersion": 24,
        "targetSdkVersion": 33,
        "permissions": ["INTERNET"]
      }
    },
    "allowlist": {
      "all": false,
      "window": {
        "all": false,
        "close": true,
        "hide": true,
        "maximize": true,
        "minimize": true,
        "show": true,
        "unmaximize": true,
        "unminimize": true
      },
      "process": {
        "all": false,
        "exit": true,
        "relaunch": true
      },
      "shell": {
        "all": false,
        "open": true
      },
      "fs": {
        "all": false,
        "readFile": true,
        "writeFile": true,
        "readDir": true,
        "copyFile": true,
        "createDir": true,
        "removeDir": true,
        "removeFile": true,
        "renameFile": true,
        "scope": [
          "$HOME/**",
          "$APPDATA/**",
          "$LOCALAPPDATA/**",
          "$USERPROFILE/**"
        ]
      },
      "path": {
        "all": false,
        "resolve": true,
        "join": true
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "easytier-gui",
        "width": 800
      }
    ]
  }
}
